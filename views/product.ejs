              width: "0%",
              duration: 0.2,
              ease: Linear.easeInOut,
            })
            .to(
              ".hamburgerlines .line1",
              {
                rotate: "45deg",
                duration: 0.2,

                ease: Linear.easeInOut,
              },
              "a"
            )
