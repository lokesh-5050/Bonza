      var sbar = 0;
      function sidebaropen() {
        if (sbar === 0) {
          let tl = gsap.timeline();
          tl.to(
            ".sidebarmain",
            {
              display: "flex",
            },
            "a"
          ).to(
            ".sidebar",
            {
              duration: 0.2,
              ease: Linear.easeInOut,
              x: 0,
            },
            "a"
          );

          let tl2 = gsap.timeline();
          tl2
