    <script>
      function openBody(elem, e){
        var pos = e*document.querySelector(".body").offsetWidth
        // console.log(e*document.querySelector(".body").offsetWidth, e);
        document.querySelector(".body").scrollLeft = pos;
      }



      function scrolly(){
        const body = document.querySelector(".body");
        if(body.scrollLeft >= -10 && body.scrollLeft < body.offsetWidth){
          document.querySelector(".head > h4:nth-child(1)").classList.add("active");
          document.querySelector(".head > h4:nth-child(2)").classList.remove("active");
          document.querySelector(".head > h4:nth-child(3)").classList.remove("active");
        }
        else if(body.scrollLeft > body.offsetWidth-10 && body.scrollLeft < body.offsetWidth*2){
          document.querySelector(".head > h4:nth-child(2)").classList.add("active");
          document.querySelector(".head > h4:nth-child(1)").classList.remove("active");
          document.querySelector(".head > h4:nth-child(3)").classList.remove("active");
        }
        else if(body.scrollLeft >= body.offsetWidth*2 -10 && body.scrollLeft < body.offsetWidth*3){
          document.querySelector(".head > h4:nth-child(3)").classList.add("active");
          document.querySelector(".head > h4:nth-child(1)").classList.remove("active");
          document.querySelector(".head > h4:nth-child(2)").classList.remove("active");
        }
      }
   
      const imgwrapper = document.querySelector(".imgwrapper");
      const img = document.querySelectorAll(".imgwrapper .imgs img");
      const imgs = document.querySelector(".imgwrapper .imgs");
      const slidebtn = document.querySelectorAll(".slidebtn");
      let current = 0;
      const count = img.length;
      setInterval(() => {
        current++;

        if (current >= count) {
          gsap.to(imgs, { x: 0 });
          current = 0;
        } else {
          gsap.to(imgs, { x:`-${100 * (current)}%` });
        }
      }, 2000);

      function slide(direc) {
        if (direc == "right") {
          current++;

          if (current >= count) {
            gsap.to(imgs, { x: 0 });
            current = 0;
          } else {
            gsap.to(imgs, { x: `-${100 * (current)}%`  });
          }
        } else {
          current = current - 1;

          if (current <= 0) {
            gsap.to(imgs, { x: `-${100 * (count - 1)}%` });
            current = count;
          } else {
            gsap.to(imgs, { x: `-${100 * (current-1)}%`  });
          }
        }
      }


      let flag = 0;
